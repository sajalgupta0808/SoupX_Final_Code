<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>SoupX</title>
  <link rel="shortcut icon" type="image/png" href="../assets/images/logos/soupxlogo-transparent.png" />
  <link rel="stylesheet" href="/soupx/admin/assets/css/styles.min.css" />
  <link rel="stylesheet" href="/soupx/admin/assets/css/subscription.css">
</head>

<body>
  <!--  Body Wrapper -->
  <div class="page-wrapper" id="main-wrapper" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full"
    data-sidebar-position="fixed" data-header-position="fixed">
    <!-- Sidebar Start -->
    <aside class="left-sidebar">
      <!-- Sidebar scroll-->
      <div>
        <div class="brand-logo d-flex align-items-center justify-content-between">
          <a href="/admin" class="text-nowrap logo-img">
            <img src="/soupx/admin/assets/images/logos/soupxlogo-transparent.png" width="180" alt="" />
          </a>
          <div class="close-btn d-xl-none d-block sidebartoggler cursor-pointer" id="sidebarCollapse">
            <i class="ti ti-x fs-8"></i>
          </div>
        </div>
        <!-- Sidebar navigation-->
        <nav class="sidebar-nav scroll-sidebar" data-simplebar="">
          <ul id="sidebarnav">
            <li class="nav-small-cap">
              <i class="ti ti-dots nav-small-cap-icon fs-4"></i>
              <span class="hide-menu">Home</span>
            </li>
            <li class="sidebar-item">
              <a class="sidebar-link" href="/admin/leads" aria-expanded="false">
                <span>
                  <i class="ti ti-layout-dashboard"></i>
                </span>
                <span class="hide-menu">Explore Leads</span>
              </a>
            </li>

            <li class="sidebar-item">
              <a class="sidebar-link" href="subscription" aria-expanded="false">
                <span>
                  <i class="ti ti-article"></i>
                </span>
                <span class="hide-menu">Subscription</span>
              </a>
            </li>
            <li class="sidebar-item">
              <a class="sidebar-link" href="/admin" aria-expanded="false">
                <span>
                  <i class="ti ti-x"></i>
                </span>
                <span class="hide-menu">LogOut</span>
              </a>
            </li>
        </nav>
        <!-- End Sidebar navigation -->
      </div>
      <!-- End Sidebar scroll-->
    </aside>
    <!--  Sidebar End -->
    <!--  Main wrapper -->
    <div class="body-wrapper">

      <div class="container-fluid">

        <div class="row">
          <div class="col-lg-8 d-flex align-items-stretch">
            <div class="card w-100">
              <div class="card-body p-4">
                <h5 class="card-title fw-semibold mb-4">Subscription Customers</h5>
                <div class="table-responsive">
                  <div class="d-flex" style="justify-content: center;">
                    <div class="loader"></div>
                  </div>
                  <table id="sub-table" class="table text-nowrap mb-0 align-middle hidden">
                    <thead class="text-dark fs-4">
                      <tr>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Id</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Name</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Phone</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Sex</h6>
                        </th>
                        <!-- <th class="border-bottom-0"> -->
                        <!-- <h6 class="fw-semibold mb-0">Age</h6> -->
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Amount</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Payment ID</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Pay Later</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Read More</h6>
                        </th>


                      </tr>
                    </thead>
                    <tbody id="sub-body">
                      <!-- <tr> -->
                      <!-- <td class="border-bottom-0"><h6 id="id" class="fw-semibold mb-0">1</h6></td>
                        <td class="border-bottom-0">
                            <h6 class="fw-semibold mb-1" id="name">Sunil Joshi</h6> -->
                      <!-- <span class="fw-normal">Web Designer</span>                           -->
                      <!-- </td>
                        <td class="border-bottom-0">
                          <p class="mb-0 fw-normal" id="phone">Elite Admin</p>
                        </td>
                        <td class="border-bottom-0">
                          <div class="d-flex align-items-center gap-2">
                            <span class="badge bg-primary rounded-3 fw-semibold" id="sex"></span>
                          </div>
                        </td>
                        <td class="border-bottom-0">
                          <h6 class="fw-semibold mb-0 fs-4" id="age"></h6>
                        </td>
                      </tr>  -->
                    </tbody>
                  </table>
                  <div id="myModal" class="modal">
                    <!-- Modal content -->
                    <div class="modal-content">
                      <span class="close" onclick="closeModal()">&times;</span>
                      <h2 id="modal-title">Your Modal Title</h2>
                      <div class="modal-body">
                        <table class="table">
                          <tr>
                            <th>Name:</th>
                            <td><span id="s_name"></span></td>
                          </tr>
                          <tr>
                            <th>Phone:</th>
                            <td><span id="s_phone"></span></td>
                          </tr>
                          <tr>
                            <th>Address</th>
                            <td><span id="s_address"></span></td>
                          </tr>
                          <tr>
                            <th>City:</th>
                            <td><span id="s_city"></span></td>
                          </tr>
                          <tr>
                            <th>PIN Code:</th>
                            <td><span id="s_pincode"></span></td>
                          </tr>
                          <tr>
                            <th>Goal:</th>
                            <td><span id="s_goal"></span></td>
                          </tr>
                          <tr>
                            <th>Gender</th>
                            <td><span id="s_gender"></span></td>
                          </tr>
                          <tr>
                            <th>Preferences:</th>
                            <td><span id="s_prefs"></span></td>
                          </tr>
                          <tr>
                            <th>Selected Plan:</th>
                            <td><span id="s_plan_name"></span></td>
                          </tr>
                          <tr>
                            <th>Plan Price:</th>
                            <td><span id="s_plan_price"></span></td>
                          </tr>
                          <tr>
                            <th>Addons:</th>
                            <td><span id="s_plan_addons">None</span></td>
                          </tr>
                          <tr>
                            <th>Selected Meals:</th>
                            <td><span id="s_meals">Soup-Lunch</span></td>
                          </tr>
                          <tr>
                            <th>Selected Days:</th>
                            <td><span id="s_days"></span></td>
                          </tr>
                        </table>
                      </div>
                      <div class="d-flex" style="justify-content: flex-end;">
                        <button onclick="closeModal()" class="modal-btn"><strong>Close</strong></button>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>

        <div class="py-6 px-6 text-center">
          <!-- <p class="mb-0 fs-4">Design and Developed by Sajal Gupta</p> -->
        </div>
      </div>
    </div>
  </div>
  <script src="/soupx//admin//assets/libs/jquery/dist/jquery.min.js"></script>
  <script src="../assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <script src="../assets/js/sidebarmenu.js"></script>
  <script src="../assets/js/app.min.js"></script>
  <script src="/soupx/admin/assets/js/subscription.js"></script>
  <script src="../assets/libs/apexcharts/dist/apexcharts.min.js"></script>
  <script src="../assets/libs/simplebar/dist/simplebar.js"></script>
  <script src="../assets/js/dashboard.js"></script>
</body>

</html>